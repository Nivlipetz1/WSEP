<Window x:Class="GUI.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:GUI"
        Closing="Window_Closing"
        mc:Ignorable="d"
        Icon="./Images/icon.ico"
        Title="BGU Poker" ResizeMode="CanMinimize" Height="600" Width="825">
    <Grid Background="Black" Margin="2,0,-2,0">
        <Grid.RowDefinitions>
            <RowDefinition Height="31*"/>
            <RowDefinition Height="541*"/>
        </Grid.RowDefinitions>
        <Frame Name="statusFrame" Grid.Row="0" NavigationUIVisibility="Hidden"/>
        <Frame Content="" Name="mainFrame" Grid.Row="1" Background="AliceBlue" NavigationUIVisibility="Hidden" />
    </Grid>
</Window>
