<Page x:Class="GUI.ReplayGamePlayback"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      mc:Ignorable="d" 
      d:DesignHeight="569" d:DesignWidth="525"
	Title="ReplayGamePlayback">

    <Canvas>
        <Canvas.Background>
            <ImageBrush ImageSource="./Images/gamebackground.png" Stretch="UniformToFill"/>
        </Canvas.Background>
        <Button Name="Start" Content="Start Replay" Width="75" Canvas.Left="219" Canvas.Top="24" Click="Start_Click"/>
        <Button Name="Back" Content="Go Back" Width="75" Canvas.Left="28" Canvas.Top="252" Click="Back_Click"/>
    </Canvas>
</Page>
